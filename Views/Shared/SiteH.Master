	<asp:ContentPlaceHolder ID="MainContent" runat="server" />
	<footer id="footer">
		<div class="container">
			<div class="row">
				<div class="col-xs-12">
					&copy; Енерготрансзахід -
					<%=DateTime.Now.Year %>
