	<div class="header_content">

		<div class="top">
			<div class="container">
				<div class="row">
					<div class="logo col-xs-12">
						<a class="img-responsive" href="<%=Url.Action("Index", "Home") %>" title="ПП Енерготрансзахід">
							<img alt="ПП Енерготрансзахід" src="<%=Url.Content("~/Content/Images/logo.png") %>" />
						</a>
					</div>
				</div>
			</div>
		</div>
