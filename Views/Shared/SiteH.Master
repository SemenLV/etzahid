									<ul class="dropdown-menu wow fadeIn" aria-labelledby="ddlClients">
										<li>
											<a href="<%=Url.Action("Tariffs", "Documents") %>" title="Тарифи">
												Тарифи</a>
										</li>
										<li>
											<a href="<%=Url.Action("Index", "Appeal") %>" title="Звернення">
												Звернення</a>
										</li>
										<li>
